@namespace AntdMangement.Components

<div class="linkGroup">
    @foreach (var link in Links)
    {
        <a href="@link.Href">@link.Title</a>
    }

    <Button Size="small" Type="primary" Ghost>
        <Icon Type="plus" /> Add
    </Button>
</div>
@code
{
    [Parameter] public EditableLink[] Links { get; set; }

}